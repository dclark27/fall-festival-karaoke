generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Signup {
  id        String   @id @default(cuid())
  name      String
  song      String
  artist    String
  createdAt DateTime @default(now())
  order     Int      @default(autoincrement())
}